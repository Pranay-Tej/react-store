{
  "name": "react-store",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "format": "prettier --write .",
    "netlify-functions:serve": "netlify functions:serve",
    "localtunnel": "lt --port 9999 --subdomain store-hasura",
    "netlify-dev": "netlify dev",
    "netlify-dev:live": "netlify dev --live",
    "netlify-env:import": "netlify env:import .env.local",
    "codegen": "graphql-codegen --config graphql.config.yaml --errors-only"
  },
  "dependencies": {
    "@mantine/core": "^4.2.10",
    "@mantine/hooks": "^4.2.10",
    "@netlify/functions": "^1.0.0",
    "@tanstack/react-query": "^4.2.1",
    "@tanstack/react-query-devtools": "^4.2.1",
    "axios": "^0.23.0",
    "date-fns": "^2.28.0",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hook-form": "^7.18.1",
    "react-router-dom": "^5.3.0",
    "tabler-icons-react": "^1.50.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-query": "^4.0.1",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/axios": "^0.14.0",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.1",
    "@vitejs/plugin-react": "^1.0.0",
    "autoprefixer": "^10.3.7",
    "postcss": "^8.3.11",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  }
}
